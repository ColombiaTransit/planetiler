{
  "problemMatcher": [
    {
      "owner": "javac",
      "pattern": [
        {
          "regexp": "^\\[([^\\]]*)\\] ([^:]*):\\[([0-9]+),([0-9]*)\\] \\[([^\\]]*)\\] (.*)$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "column": 4,
          "code": 5,
          "message": 6
        },
        {
          "regexp": "^\\s+(.*)$",
          "message": 1,
          "loop": true
        }
      ]
    }
  ]
}
